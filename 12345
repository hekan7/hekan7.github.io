package bean.user;

public class homework2 {
	private int sum;
	private int inputNum;
	
	public void setInputNum(int inputNum) { this.inputNum=inputNum; }
	public int getInputNum() { return inputNum; }
	public int getsum() {
		int i=1, sum=0;
		while(i<=inputNum) {
			sum += i++;
		}
		
		return sum;
	}
}

---------
package bean.user;

public class homework2 {
	private int sum;
	private int inputNum;
	
	public void setInputNum(int inputNum) { this.inputNum=inputNum; }
	public int getInputNum() { return inputNum; }
	public int getsum() {
		int i=1, sum=0;
		while(i<=inputNum) {
			sum += i++;
		}
		
		return sum;
	}
}


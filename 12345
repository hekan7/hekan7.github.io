package bean.user;

public class homework1 {
	private int num1, num2;
	private int startNum;
	private int endNum;
	private int max, min;
	
	public void setStartNum(int startNum) { this.startNum=startNum; }
	public int getStartNum() { return startNum; }
	public void setEndNum(int endNum) { this.endNum=endNum; }
	public int getEndNum() { return endNum; }
	public int getNum1() {
		num1=(int)(Math.random()*(endNum-startNum))+startNum;
		return num1;
	}
	public int getNum2() {
		num2=(int)(Math.random()*(endNum-startNum))+startNum;
		return num2;
	}
	public int getmax() {
		if(num1>num2) {
			max=num1;
		} else if (num2>num1) {
			max=num2;
		}
		return max;
	}
	public int getmin() {
		if(max==num2) {
			min=num1;
		} else if (max==num1) {
			min=num2;
		}
		return min;
	}
}


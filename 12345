-----------
정수 2개(ex 10, 100) 입력하고, 입력된 작은 정수 값에서부터 정수 큰 값까지 합계 인쇄

private int num1, num2;
private int sum, min, max;

public void setNum1(int num1) { this.num1=num1; }
public int getNum1() { return num1; }
public void setNum2(int num2) { this.num2=num2; }
public int getNum2() { return num2; }

public int getmax() {
	if(num1>num2) {
		max=num1;
	} else if (num2>num1) {
		max=num2;
	}
	return max;
}
public int getmin() {
	if(max==num1) {
		min=num2;
	} else {
		min=num1;
	}
	return min;
}
	
public int sum() {
		sum=0;
		while(min<=max) {
			sum += min++;
		}
		
		return sum;
	}

public int getsum() {
		int i=1, sum=0;
		while(i<=max) { sum += i++; }
		return sum;
	}
